@model List<_200393765_quiz.Models.Car>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<table>
    <tr>
        <th>ID</th>
        <th>Brand</th>
        <th>Model</th>
        <th>Type</th>
        <th>Year</th>
    </tr>
    @foreach (var car in Model)
    {

        <tr>
            <td>@car.ID</td>
            <td>@car.Brand</td>
            <td>@car.Model</td>
            <td>@car.Type</td>
            <td>@car.Year</td>
        </tr>
    }
</table>